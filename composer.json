{
    "name": "nlybe/geomaps_api",
    "description": "The Geo Maps plugin offers an API with PHP/JS libraries for the creation geo-visualization maps of Elgg entities",
    "type": "elgg-plugin",
    "keywords": [
        "elgg", 
        "plugin",
        "maps",
        "geo"
    ],
    "homepage": "https://github.com/nlybe/elgg_geomaps_api",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Nikos Lyberakis",
            "email": "nikos@lyberakis.gr",
            "role": "Web Developer"
        }
    ],
    "require": {
        "composer/installers": ">=1.0.8",
        "willdurand/geocoder": "@stable",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/message": "^1.7",
        "geocoder-php/chain-provider": "^4.0",
        "geocoder-php/google-maps-provider": "^4.0"
    },
    "extra": {
        "installer-name": "geomaps_api"
    },
	"conflict": {
		"elgg/elgg": "<4.3"
	}
}